{
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
  "version": 3,
  "slug": "section-accommodation-facts",
  "title": "Accommodation Fast Facts Section",
  "blockTypes": [
    "core/group",
    "core/columns",
    "core/column"
  ],
  "styles": {
    "color": {
      "background": "var:preset|color|accent-2",
      "text": "var:preset|color|contrast"
    },
    "spacing": {
      "padding": {
        "top": "var:preset|spacing|30",
        "bottom": "var:preset|spacing|30"
      }
    },
    "blocks": {
      "core/separator": {
        "color": {
          "text": "color-mix(in srgb, currentColor 25%, transparent)"
        }
      }
    },
    "elements": {
      "button": {
        "color": {
          "background": "var:preset|color|contrast",
          "text": "var:preset|color|accent-2"
        },
        ":hover": {
          "color": {
            "background": "color-mix(in srgb, var(--wp--preset--color--contrast) 80%, transparent)",
            "text": "var:preset|color|accent-2"
          }
        }
      }
    }
  }
}